<script>
var date
function openForm(b) {
if(b==1)
  document.getElementById("open?").style.display="none";
  document.getElementById("asd").style.display="block";
}

function asd(a) {
if(a==2)
  document.getElementById("open?").style.display="block";
  document.getElementById("asd").style.display="none";

if(a==3)
  document.getElementById("open?").style.display="block";
  document.getElementById("open?").style.display="block";
}



</script>


    
  

